<div class="page-header">
  <h1>Add Data</h1>
</div>

<%= form_for(@page, url: admin_create_page_data_path(@page.id), method: :post) do |f| %>
    <div class="row">
      <div class="large-6 columns">
        <%= f.label :key, class: 'control-label' %>
        <%= f.text_field :key, class: 'form-control' %>
      </div>
    </div>
    <div class="row">
      <div class="large-6 columns">
        <%= f.label :value, class: 'control-label' %>
        <%= f.text_field :value, class: 'form-control' %>
      </div>
    </div>
    <%= f.submit 'Submit', id: 'submit', class: 'button success small' %>
<% end %>
