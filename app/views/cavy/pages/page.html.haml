%p#notice= notice

%h1#title{"data-mercury" => "full"}
  = @page.title if @page

%p
  %strong Content:
  #content{"data-mercury" => "full"}
    = @page.content.html_safe if @page

%p
  %strong test:
  %div{id: "test_#{I18n.locale}", "data-mercury" => "full"}
    - if @page != nil
      - if @page.page_elements["test_#{I18n.locale}"] != nil
        = @page.page_elements["test_#{I18n.locale}"].html_safe